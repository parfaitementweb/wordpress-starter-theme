{
    "name": "parfaitementweb/wordpress-starter-theme",
    "require": {
        "parfaitementweb/wordpress-starter-core": "1.1.*",
        "wpackagist-plugin/classic-editor": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "extra": {
        "installer-paths": {
            "../../plugins/{$name}": [
                "wpackagist-plugin/classic-editor"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
